DBFind(@1_buffer_data, src_buffer).Columns("value->app_id").Where({key:export, member_id:#key_id#, ecosystem:#ecosystem_id#}).Vars(buffer)
If(#buffer_value_app_id# > 0){
	DBFind(@1_applications, src_app).Where({id:#buffer_value_app_id#,ecosystem:#ecosystem_id#}).Vars("application")

	Div(content-wrapper){
		SetTitle("Binary data": #application_name#)
		AddToolButton(Title: "Upload binary", Page: @1app_upload_binary, Icon: icon-plus, PageParams: "application_id=#application_id#")

		SetVar(pager_table, binaries).(pager_where, "{app_id:#buffer_value_app_id#,ecosystem:#ecosystem_id#}").(pager_page, @1app_binary).(pager_limit, 50)
		Include(pager_header)

		SetVar(admin_page, @1app_binary)
		SetVar(hideLink, 0)
		Include(admin_link)

		DBFind(@1_binaries, src_binparameters).Limit(#pager_limit#).Count(cnt).Order(#sort_name#).Offset(#pager_offset#).Where({app_id:#buffer_value_app_id#,ecosystem:#ecosystem_id#}).Custom(_name){
			LinkPage(Page: @1app_upload_binary, PageParams: "id=#id#,application_id=#buffer_value_app_id#", Body: #name#)
		}.Custom(_img){
			Div(pull-right){
				Image(Src: Binary().ById(#id#), Class: preview).Style(height: 40px; widht 40px;)
			}
		}

		Form(panel panel-primary){
			Div(panel-body){
				Div(row){
					Div(col-md-#width# col-sm-12){
						If(#cnt#>0){
							Div(list-group-item){
								Div(row){
									Div(col-md-12){
										Table(src_binparameters,"ID=id,Name=_name,Download link=data,=_img")
									}
								}
							}
						}
					}
				}
			}
			Div(panel-footer clearfix){
				Include(pager)
			}
		}
	}
}.Else{
	SetTitle("Binary data")
	Div(breadcrumb){
		Span(Class: text-muted, Body: "You did not select the application. Viewing resources is not available")
	}
}