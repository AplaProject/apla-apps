contract ItemChangeAppId {
    data {
        Table string
        AppId int
        Id int
    }

    conditions {
        var tbl string
        tbl = "@1_" + Str($Table)
        $item = DBFind(tbl).Where({id: $Id, ecosystem: $ecosystem_id}).Row()
        if !$item{
            warning "Item not found"
        }
    }

    action {
        DBUpdate($Table, Int($item["id"]), {"app_id": $AppId})
    }
}