contract land_registry_Depart3Edit{
    data{
        LandId int
        NotificId int
        TaxReceiptAndBills int
        PriceMarket int
        PriceGov int
    }
    
    func action{
        DBUpdate("land_registry", $LandId, {"tax_receipt_and_bills": $TaxReceiptAndBills, "price_market_value": $PriceMarket, "price_gov_value": $PriceGov, "modification_date": $block_time})
        
        @1NotificationsClose("notific_id", $NotificId)
    }
}