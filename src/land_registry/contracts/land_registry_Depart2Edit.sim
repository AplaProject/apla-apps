contract land_registry_Depart2Edit{
    data{
        LandId int
        NotificId int
        LegalStatus int
        EncumbranceCertificate int
    }
    
    func action{
        DBUpdate("land_registry", $LandId, {"legal_status": $LegalStatus, "encumbrance_certificate": $EncumbranceCertificate, "modification_date": $block_time})
        
        @1NotificationsClose("notific_id", $NotificId)
    }
}