SetTitle($land_bank$)
DBFind(@1applications).Where({ecosystem:1, name:"Land Registry"}).Columns("name,id").Vars(app)

DBFind("land_registry_ownership").Where({"step": 1}).Vars(step1)

If(GetVar(step1_lend_object_id)){
    SetVar(LandId, #step1_lend_object_id#)
}.Else{
    SetVar(LandId, 0)
}

If(GetVar(LandId)){
    DBFind("land_registry").WhereId(#LandId#).Vars(entity)
    DBFind("land_registry_ownership").Where({"lend_object_id": #LandId#}).Vars(ownership)
    DBFind("@1members").Where({"id": #entity_owner_id#, "ecosystem": #ecosystem_id#}).Vars(owner)
    DBFind("@1members").Where({"id": #ownership_owner_new_id#, "ecosystem": #ecosystem_id#}).Vars(buyer)

    Div(content-wrapper){
        Div(row df f-valign){
            Div(col-sm-8 col-sm-offset-2 col-lg-6 col-lg-offset-3){
                Div(panel panel-primary data-sweet-alert){
                    Div(panel-heading){
                        Div(panel-title){
                            Land Record #GetVar(LandId) Data
                        }
                    }
                    Form(){
                        Div(list-group-item){
                            Div(row df f-valign){
                                Div(col-md-5 mt-sm text-right){
                                    Div(h5 text-normal){
                                        $area_size$:
                                    }
                                }
                                Div(col-md-7 mt-sm text-left) {
                                    Div(h5 text-normal){GetVar(entity_area) Span(m).(2).Style(vertical-align: super; font-size:8px;)}
                                }
                            }
                            Div(row df f-valign){
                                Div(col-md-5 mt-sm text-right){
                                    Div(h5 text-normal){ LangRes(address):}
                                }
                                Div(col-md-7 mt-sm text-left){
                                    Div(h5 text-normal){ GetVar(entity_address)}
