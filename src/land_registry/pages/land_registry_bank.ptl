                                    If(#entity_legal_status#>0){
                                        Div(h5 text-normal){
                                            AppParam(App: #app_id#, Name: land_yes_no, Index: #entity_legal_status#)
                                        }
                                    }.Else{
                                        Div(h5 text-normal){ LangRes(undefined)}
                                    }
                                }
