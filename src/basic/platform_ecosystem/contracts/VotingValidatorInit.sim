contract VotingValidatorInit{
    data{
        KeyId int
    }
    
    conditions{
        ContractConditions("MainCondition")
        $candidate = DBFind("sysparams_candidate").Where("candidate_key_id=? and deleted=0", $KeyId).Row()
        if !$candidate{
            warning "Candidate not found"
        }
    }
    action{
        DelegateRefresh()
    }
}