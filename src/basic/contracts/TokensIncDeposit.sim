contract TokensIncDeposit {
    data {
        Amount money
    }
    conditions {
        $amount = DBFind("@1keys").WhereId($key_id).One("amount")
        if Money($Amount) > Money($amount) {
            info("Insufficient funds")
        }
    }
    action {
        var s map
        s["+deposit"] = $Amount
        DBUpdate("@1keys",$key_id,s)
    }
}