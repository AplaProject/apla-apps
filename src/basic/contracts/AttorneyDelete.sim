contract AttorneyDelete {
    data {
        Id int
    }
    conditions {
        $PowerOfAttorney = DBRow("power_of_attorney").Where({"id": $Id, "address": $key_id})
        if !$PowerOfAttorney {
            warning "Power of attorney does not exists"
        }
    }
    action {
        DBUpdate("power_of_attorney", $Id, {"delete": 1})
    }
}