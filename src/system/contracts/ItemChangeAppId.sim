contract ItemChangeAppId {
    data {
        Table string
        AppId int
        Id int
    }

    conditions {
        $item = DBFind($Table).Where({"id": $Id, "ecosystem": $ecosystem_id}).Row()
        if !$item{
            warning LangRes("@1item_not_found", "en")
        }
    }

    action {
        DBUpdate($Table, Int($item["id"]), {"app_id": $AppId})
    }
}