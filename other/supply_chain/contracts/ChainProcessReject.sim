contract ChainProcessReject {
    data {
        ChainID int
    }
    conditions {
        var admin int
        admin = Int(EcosysParam("role_admin"))
        if !RoleAccess(admin){
            warning LangRes("@1access_denied", "en")
        }
    }
    action {
        var m map
        m["status"] = 4
        DBUpdate("@1supply_chains", $ChainID, m)
        @1NotificationsClose("notific_id", $notific_id)
    }
}