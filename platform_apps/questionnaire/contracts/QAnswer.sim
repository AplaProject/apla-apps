contract QAnswer {
    data {
        question_id int
        decision string
        answer_id int
    }
    conditions {
        //question exist && status
    }
    action {
        var a map
        a["member"] = $key_id
        a["decision"] = $decision
        a["answer_id"] = $answer_id
        a["ecosystem"] = $ecosystem_id
        a["lastmodified"] = $time
        a["question_id"] = $question_id
        //warning Sprintf("%v", a)
        DBInsert("@1q_answers", a)
    }
}